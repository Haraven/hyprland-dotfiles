    "backlight": {
        "device": "intel_backlight",
        "rotate": ${r_deg},
        "format": "{percent}% {icon}",
        "tooltip": "{Brightness {percent}%}",
        "format-icons": ["", "", "", "", "", "", "", "", ""],
        "on-scroll-up": "brightnessctl set 1%+",
        "on-scroll-down": "brightnessctl set 1%-",
        "min-length": 6
    },

